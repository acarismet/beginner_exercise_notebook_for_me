print("\n***\nFunctions_Sub_Multiple\n***")

def sub_multi(num):
    for i in range(2,num):
        if (num % i == 0):
           return i

while True:
  num = input("Type Your Num You İ.Melih G:")

  if (num == "q"):
     print("See yea mate")
     break
  else:
     num = int(num)
     print(sub_multi(num))

print("\nHocanın Yöntemi\n")

def tambolenleribulma(sayı):
    tam_bolenler = []

    for i in range(2,sayı):

        if (sayı % i == 0):
            tam_bolenler.append(i)
    return tam_bolenler

while True:

    sayı = input("Sayı:")

    if (sayı == "q"):
        print("Program Sonlandırılıyor")
        break
    else:
        sayı = int(sayı)

        print("Tam bölenler:",tambolenleribulma(sayı))
